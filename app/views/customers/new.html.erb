<h1>Customers</h1>

<%= form_for @customer do |f| %>
	<%= f.text_field :email, placeholder: "your super secret email address..  we will be spamming.." %>
	<%= f.text_field :address, placeholder: "where can i send you snail mail?" %>
	<%= f.text_field :password, placeholder: "password ..." %>
	<%= f.submit %>
<% end %>